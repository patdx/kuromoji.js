{
	"include": ["./src/**/*.js", "./test/**/*"],
	"compilerOptions": {
		"allowJs": true,
		"strict": true,
		"noImplicitAny": false,
		"noEmit": true,
		"moduleResolution": "bundler",
		"target": "ESNext",
		"types": ["node", "vitest/globals"],
		"skipLibCheck": true,
		"verbatimModuleSyntax": true
	}
}
